{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "curly": 2,
    "no-unused-vars": 2,
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "linebreak-style": "off",
    "eqeqeq": 2,
    "no-prototype-builtins": "off",
    "require-await": "error"
  }
}
